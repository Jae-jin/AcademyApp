<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Studystart_review">
    <com.ssomai.android.scalablelayout.ScalableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:scale_base_width="1000"
        app:scale_base_height="1600">
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_left="80"
            app:scale_width="840"
            app:scale_height="300"
            app:scale_top="400"
            android:text="Start Review"
            app:scale_textsize="90"
            tools:ignore="MissingPrefix"
            android:id="@+id/studyreview"
            android:background="#1e90ff"
            android:textColor="#000000"
            />
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:scale_left="80"
            app:scale_width="840"
            app:scale_height="300"
            app:scale_top="900"
            android:text="복습단어 초기화"
            app:scale_textsize="90"
            tools:ignore="MissingPrefix"
            android:id="@+id/clearreview"
            android:background="#1e90ff"
            android:textColor="#000000"
            />
    </com.ssomai.android.scalablelayout.ScalableLayout>
</android.support.constraint.ConstraintLayout>